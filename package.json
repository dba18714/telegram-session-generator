{
  "name": "telegram-session-generator",
  "version": "1.5.2",
  "description": "ðŸš€ A secure and easy-to-use CLI tool for generating Telegram session strings. Perfect for Telegram client development and automation projects.",
  "bin": {
    "telegram-session": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "docker:build": "docker build -t telegram-session-generator .",
    "docker:run": "docker run --rm -it telegram-session-generator",
    "docker:build-multi": "docker buildx build --platform linux/amd64,linux/arm64 -t telegram-session-generator .",
    "docker:test": "docker run --rm telegram-session-generator --help"
  },
  "keywords": [
    "telegram",
    "session",
    "cli",
    "tool",
    "authentication",
    "client",
    "generator",
    "automation",
    "nodejs",
    "typescript",
    "telegram-client",
    "session-string"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dba18714/telegram-session-generator.git"
  },
  "bugs": {
    "url": "https://github.com/dba18714/telegram-session-generator/issues"
  },
  "homepage": "https://github.com/dba18714/telegram-session-generator#readme",
  "dependencies": {
    "telegram": "^2.26.22"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.9",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
